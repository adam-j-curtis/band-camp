<h1>Users Index</h1>
</br>
<h3>Users:</h3>
  <% @users.each do |user| %>
  <h6><%= link_to user.full_name, user_path(user) %></h6>
<% end %>
<br/>

<h3 class="students-heading">Students:</h3>
  <% @students.each do |student| %>
  <h6 class="student-name"><%= link_to student.full_name, user_path(student) %></h6>
  <% end %>
